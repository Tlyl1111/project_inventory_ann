<odoo>
    <record id="view_stock_ann_forecast_graph" model="ir.ui.view">
        <field name="name">stock.ann.forecast.graph</field>
        <field name="model">stock.ann.forecast</field>
        <field name="arch" type="xml">
            <graph string="Biểu đồ dự báo tồn kho (ANN)" type="line" sample="1">
                <field name="prediction_date" interval="week"/>
                <field name="product_id"/>
                <field name="predicted_qty" type="measure"/>
            </graph>
        </field>
    </record>
</odoo>
